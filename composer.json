{
    "name": "storinka/invoke",
    "description": "Invoke Core library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Davyd Kohut",
            "email": "expexes@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "autoload": {
        "psr-4": {
            "Invoke\\": "src"
        },
        "files": [
            "src/utils.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "InvokeTests\\": "tests/lib",
            "InvokeTests\\Typesystem\\": "tests/typesystem"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    }
}
