{
    "name": "storinka/invoke",
    "description": "Invoke Core library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Davyd Kohut",
            "email": "expexes@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-mbstring": "*",
        "composer/semver": "^3.2",
        "php-ds/php-ds": "^1.4",
        "psr/container": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Invoke\\": "src"
        },
        "files": [
            "src/utils.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "InvokeTests\\": "tests",
            "InvokeTests\\Lib\\": "tests/lib",
            "InvokeTests\\Typesystem\\": "tests/typesystem",
            "InvokeTests\\Invoke\\": "tests/invoke"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    }
}
